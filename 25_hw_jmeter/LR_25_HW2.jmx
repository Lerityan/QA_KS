          <stringProp name="script">import org.apache.jmeter.config.Argument;  //этот фрагмент
import org.apache.jmeter.config.Arguments; //я нашел на стаковерфлоу
double test = 0; 						//не уверен на счет объявления переменных ВНУТРИ массива
//double test2 = 0;
Arguments argz = ctx.getCurrentSampler().getArguments();  //получаем массив аргументов
for (int i = 0; i &lt; argz.getArgumentCount(); i++) {		//перебираем каждый элемент массива
    Argument arg = argz.getArgument(i);			// получаем  аргумент

    if (arg.getName().equals(&quot;weight&quot;)) {			//если нужный нам аргумент		
	    test = Double.parseDouble(arg.getValue())*0.012;     //я до 5 часов утра искал как правильно умножать в биншел
//	    log.error(test.toString());						//и куда выводятся логи. еррор потому что инфо засрано
//	    test2  = test*0.012;
	  //  log.error(test2.toString());
      vars.put(&quot;compare_weight&quot;, test.toString() );   	  			//и оно заработало

        break;
    }
}


